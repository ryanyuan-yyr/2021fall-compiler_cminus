struct complex_num
{
    float r;
    float i;
    struct complex_num operator+(struct complex_num rhs)
    {
        struct complex_num res;
        res.r = (*this).r + rhs.r;
        res.i = (*this).i + rhs.i;
        return res;
    }
};

int main(){
    struct complex_num a;
    a.r = 1.0;
    a.i = 2.0;
    struct complex_num b;
    b.r = 3.0;
    b.i = 4.0;
    struct complex_num c;
    c = a + b;
    print_float(c.r);
    print_char(32);
    print_float(c.i);
    return 0;
}
